@model IEnumerable<pamela_soulis_project1.Domain.Model.Product>

@{
    ViewData["Title"] = "Index";
}


<h1 style="color:darkblue">Browse Our Products</h1>
<table class=" = " table">
    <thead>
        <tr>
            <th style="color:cadetblue">Product</th>
            <th style="color:cadetblue">Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Price</td>
            </tr>
        }
    </tbody>
</table>


